int seatNo =0; 
public void bill(){ 
String customer = jTextField1.getText(); 
String contact = jTextField4.getText(); 
String from = (String)jComboBox1.getSelectedItem(); 
String to = (String)jComboBox2.getSelectedItem(); 
int seat = seatNo; 
String price = jTextField3.getText(); 
SimpleDateFormat date_format = new SimpleDateFormat("yyyy-MM-dd"); 
String date = date_format.format(jCalendar2.getDate()); 
TA.setText(TA.getText() + "****************************\n"); 
TA.setText(TA.getText() + "**********BUS TICKET*********\n");
TA.setText(TA.getText() + "*****************************\n"); 
TA.setText(TA.getText() + "Customer " + "\t" + customer + "\n"); 
TA.setText(TA.getText() + "Contact Number " + "\t" + contact + "\n"); 
TA.setText(TA.getText() + "From: " + from + "To: " + to + "\n"); 
TA.setText(TA.getText() + "SeatNo " + "\t" + seat + "\n"); 
TA.setText(TA.getText() + "Price " + "\t" + price + "\n"); 
TA.setText(TA.getText() + "Date " + "\t" + date + "\n"); 
TA.setText(TA.getText() + "**********^^^^^^^^^**********\n"); 
TA.setText(TA.getText() + "******Thank You Come Again!!******\n"); 
} 
public static Date dateToday() { 
LocalDate now = LocalDate.now(); 
Date date = java.sql.Date.valueOf(now); 
return date; 
} 
